 ,xx.REF_CODE Ref_Codee,xx.ACTUAL_UNIT_PRICE ACTUAL_UNIT_PRICEe,xx.CONS_PER_PCS CONS_PER_PCSs,xx.COST_PER_PCS COST_PER_PCSs,
XX_OM_POC_D1_TEO.ACTUAL_UNIT_PRICE, 
                                  
       XX_OM_POC_D1_TEO.ADD_CHARGE_LC, 
       XX_OM_POC_D1_TEO.ADD_CRG_PRCNT, 
       XX_OM_POC_D1_TEO.ADD_DEDUCT_COST, 
       XX_OM_POC_D1_TEO.ADD_PRICE_FOB, 
       XX_OM_POC_D1_TEO.ADD_PRICE_MOQ, 
       XX_OM_POC_D1_TEO.BUFER, 
       XX_OM_POC_D1_TEO.CONS_PER_PCS, 
                                        
                                        
       XX_OM_POC_D1_TEO.CONS_WITH_WASTAGE, 
       XX_OM_POC_D1_TEO.COST_PER_PCS, 
       XX_OM_POC_D1_TEO.CREATED_BY, 
       XX_OM_POC_D1_TEO.CREATION_DATE, 
       XX_OM_POC_D1_TEO.DELIVERY_TERM_ID, 
       XX_OM_POC_D1_TEO.DETAIL_ID, 
       XX_OM_POC_D1_TEO.FAB_FINANCE_COST, 
       XX_OM_POC_D1_TEO.FINAL_CONS, 
                                      
                                         
       XX_OM_POC_D1_TEO.FINAL_COST_PER_PCS, 
       XX_OM_POC_D1_TEO.FINAL_UNIT_PRICE, 
       XX_OM_POC_D1_TEO.FOB_ID, 
       XX_OM_POC_D1_TEO.ITEM_DESC, 
       XX_OM_POC_D1_TEO.ITEM_PREFIX, 
       XX_OM_POC_D1_TEO.LAST_UPDATED_BY, 
       XX_OM_POC_D1_TEO.LAST_UPDATED_DATE, 
       XX_OM_POC_D1_TEO.PAYMENT_TERM_ID, 
       XX_OM_POC_D1_TEO.PREFIX_DESC, 
       XX_OM_POC_D1_TEO.PRICE_UOM, 
       XX_OM_POC_D1_TEO.REF_CODE, 
       XX_OM_POC_D1_TEO.REMARKS, 
       XX_OM_POC_D1_TEO.STATUS, 
       XX_OM_POC_D1_TEO.UOM, 
       XX_OM_POC_D1_TEO.UOM_CONV, 
       XX_OM_POC_D1_TEO.WASTAGE_IN_PER, 
       XX_OM_POC_D1_TEO.WASTAGE_IN_QTY,                    
        (select t.value_description
from MNJ_ITEM_PREFIX_V t
where t.flex_value = to_char(XX_OM_POC_D1_TEO.Item_Prefix))ITEM_PREFIX_DESC
FROM XX_OM_POC_D1_T XX_OM_POC_D1_TEO,QA_PLANS QP, QA_RESULTS QR,
(select Detail_id,  t.value_description ,d1.REF_CODE,d1.ACTUAL_UNIT_PRICE,d1.CONS_PER_PCS,d1.COST_PER_PCS
from MNJ_ITEM_PREFIX_V t, xx_OM_POC_D1_t d1
where t.flex_value = to_char(d1.Item_Prefix)  )xx
   WHERE    XX_OM_POC_D1_TEO.DETAIL_ID=xx.Detail_id and    XX_OM_POC_D1_TEO.ITEM_PREFIX = QR.CHARACTER2
         AND qr.plan_id = qp.plan_id
         AND UPPER (QP.NAME) = 'SEQUENCE NO POC'
        --AND XX_OM_POC_D1_TEO.ITEM_PREFIX  in (11,12)
   and  XX_OM_POC_D1_TEO.ITEM_PREFIX between 13 and 37
ORDER BY XX_OM_POC_D1_TEO.ITEM_PREFIX]]>
  </SQLQuery>
